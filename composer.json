{
    "name": "illchuk/zend-config-cacheproof",
    "description": "Create *.cacheproof.php config files that skip the cache, with custom loading.",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "zf3",
        "zend-config",
        "config",
		"cache"
    ],
    "homepage": "https://github.com/dillchuk/ZendConfigCacheproof",
    "authors": [
        {
            "name": "Derek Illchuk",
            "email": "derek@illchuk.com",
            "homepage": "http://www.derekillchuk.com"
        }
    ],
    "require": {
        "php": "^5.5|^7.0",
        "zendframework/zend-modulemanager": "^2.2",
        "zendframework/zend-servicemanager": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "ZendConfigCacheproof\\": "src/"
        }
    }
}
